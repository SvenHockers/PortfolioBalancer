{
  "dashboard": {
    "id": null,
    "title": "Portfolio Performance Analytics - Comprehensive",
    "tags": ["portfolio", "performance", "analytics", "comprehensive"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Performance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "portfolio_total_return{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Total Return",
            "refId": "A"
          },
          {
            "expr": "portfolio_annualized_return{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Annualized Return",
            "refId": "B"
          },
          {
            "expr": "portfolio_sharpe_ratio{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Sharpe Ratio",
            "refId": "C"
          },
          {
            "expr": "portfolio_volatility{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Volatility",
            "refId": "D"
          },
          {
            "expr": "portfolio_max_drawdown{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Max Drawdown",
            "refId": "E"
          },
          {
            "expr": "portfolio_calmar_ratio{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Calmar Ratio",
            "refId": "F"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "red", "value": -10},
                {"color": "yellow", "value": 0},
                {"color": "green", "value": 5}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Sharpe Ratio"},
              "properties": [
                {"id": "unit", "value": "short"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 0.5},
                    {"color": "green", "value": 1.0}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Calmar Ratio"},
              "properties": [
                {"id": "unit", "value": "short"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "yellow", "value": 0.3},
                    {"color": "green", "value": 0.5}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Cumulative Returns vs Multiple Benchmarks",
        "type": "timeseries",
        "targets": [
          {
            "expr": "portfolio_cumulative_return{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Portfolio",
            "refId": "A"
          },
          {
            "expr": "benchmark_cumulative_return{benchmark=\"SPY\"}",
            "legendFormat": "S&P 500 (SPY)",
            "refId": "B"
          },
          {
            "expr": "benchmark_cumulative_return{benchmark=\"VTI\"}",
            "legendFormat": "Total Market (VTI)",
            "refId": "C"
          },
          {
            "expr": "benchmark_cumulative_return{benchmark=\"QQQ\"}",
            "legendFormat": "NASDAQ 100 (QQQ)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "Cumulative Return (%)",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Portfolio"},
              "properties": [
                {"id": "custom.lineWidth", "value": 3},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"displayMode": "visible", "placement": "bottom", "calcs": ["lastNotNull"]}
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 3,
        "title": "Rolling Performance Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "portfolio_rolling_sharpe{portfolio_id=\"$portfolio\", window=\"30d\"}",
            "legendFormat": "30-day Sharpe",
            "refId": "A"
          },
          {
            "expr": "portfolio_rolling_volatility{portfolio_id=\"$portfolio\", window=\"30d\"}",
            "legendFormat": "30-day Volatility",
            "refId": "B"
          },
          {
            "expr": "portfolio_rolling_alpha{portfolio_id=\"$portfolio\", window=\"30d\"}",
            "legendFormat": "30-day Alpha",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineWidth": 1,
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "30-day Volatility"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "custom.axisPlacement", "value": "right"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "30-day Sharpe"},
              "properties": [
                {"id": "unit", "value": "short"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "30-day Alpha"},
              "properties": [
                {"id": "unit", "value": "percent"}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 4,
        "title": "Drawdown Analysis",
        "type": "timeseries",
        "targets": [
          {
            "expr": "portfolio_drawdown{portfolio_id=\"$portfolio\"}",
            "legendFormat": "Portfolio Drawdown",
            "refId": "A"
          },
          {
            "expr": "benchmark_drawdown{benchmark=\"SPY\"}",
            "legendFormat": "S&P 500 Drawdown",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 30,
              "gradientMode": "opacity"
            },
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": -5},
                {"color": "red", "value": -10}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        },
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 14}
      },
      {
        "id": 5,
        "title": "Excess Returns vs Benchmark",
        "type": "timeseries",
        "targets": [
          {
            "expr": "portfolio_excess_return{portfolio_id=\"$portfolio\", benchmark=\"SPY\"}",
            "legendFormat": "Excess Return vs S&P 500",
            "refId": "A"
          },
          {
            "expr": "portfolio_tracking_error{portfolio_id=\"$portfolio\", benchmark=\"SPY\"}",
            "legendFormat": "Tracking Error",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Excess Return vs S&P 500"},
              "properties": [
                {"id": "custom.fillOpacity", "value": 20},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Tracking Error"},
              "properties": [
                {"id": "custom.axisPlacement", "value": "right"},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        },
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 14}
      },
      {
        "id": 6,
        "title": "Performance Attribution Analysis",
        "type": "barchart",
        "targets": [
          {
            "expr": "portfolio_attribution_allocation{portfolio_id=\"$portfolio\", period=\"$period\"}",
            "legendFormat": "Asset Allocation Effect",
            "refId": "A"
          },
          {
            "expr": "portfolio_attribution_selection{portfolio_id=\"$portfolio\", period=\"$period\"}",
            "legendFormat": "Security Selection Effect",
            "refId": "B"
          },
          {
            "expr": "portfolio_attribution_interaction{portfolio_id=\"$portfolio\", period=\"$period\"}",
            "legendFormat": "Interaction Effect",
            "refId": "C"
          },
          {
            "expr": "portfolio_attribution_total{portfolio_id=\"$portfolio\", period=\"$period\"}",
            "legendFormat": "Total Active Return",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 3,
            "custom": {
              "orientation": "horizontal",
              "barWidth": 0.6,
              "groupWidth": 0.7
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Total Active Return"},
              "properties": [
                {"id": "custom.drawStyle", "value": "line"},
                {"id": "custom.lineWidth", "value": 3},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
              ]
            }
          ]
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 7,
        "title": "Benchmark Comparison Table",
        "type": "table",
        "targets": [
          {
            "expr": "portfolio_benchmark_comparison{portfolio_id=\"$portfolio\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "auto"
            },
            "decimals": 2
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Excess Return"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": -5},
                      {"color": "yellow", "value": 0},
                      {"color": "green", "value": 2}
                    ]
                  }
                },
                {"id": "unit", "value": "percent"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Sharpe Ratio"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": 0},
                      {"color": "yellow", "value": 0.5},
                      {"color": "green", "value": 1.0}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": false, "displayName": "Excess Return"}]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 8,
        "title": "Portfolio Allocation Drift",
        "type": "timeseries",
        "targets": [
          {
            "expr": "portfolio_allocation_drift{portfolio_id=\"$portfolio\"}",
            "legendFormat": "{{asset_class}} Drift",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 1,
              "fillOpacity": 0
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": -2},
                {"color": "yellow", "value": -5},
                {"color": "red", "value": -10}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "right"}
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 28}
      },
      {
        "id": 9,
        "title": "Performance Alerts & Notifications",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"analytics-service\"} |= \"alert\" | json | portfolio_id=\"$portfolio\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 34}
      }
    ],
    "time": {
      "from": "now-1y",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "portfolio",
          "type": "query",
          "query": "label_values(portfolio_total_return, portfolio_id)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "options": [],
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "name": "period",
          "type": "custom",
          "options": [
            {"text": "Daily", "value": "1d", "selected": false},
            {"text": "Weekly", "value": "7d", "selected": false},
            {"text": "Monthly", "value": "30d", "selected": true},
            {"text": "Quarterly", "value": "90d", "selected": false},
            {"text": "Yearly", "value": "365d", "selected": false}
          ],
          "current": {"text": "Monthly", "value": "30d"}
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Portfolio Rebalancing",
          "datasource": "Portfolio Analytics API",
          "enable": true,
          "expr": "portfolio_rebalance_events{portfolio_id=\"$portfolio\"}",
          "iconColor": "blue",
          "textFormat": "Rebalanced: {{description}}"
        },
        {
          "name": "Performance Alerts",
          "datasource": "Portfolio Analytics API", 
          "enable": true,
          "expr": "portfolio_performance_alerts{portfolio_id=\"$portfolio\"}",
          "iconColor": "red",
          "textFormat": "Alert: {{alert_type}}"
        }
      ]
    },
    "refresh": "1m",
    "schemaVersion": 30,
    "version": 1
  },
  "overwrite": true
}