{
  "dashboard": {
    "id": null,
    "title": "Backtesting Results Dashboard",
    "tags": [
      "portfolio",
      "backtesting",
      "strategy",
      "comparison"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Strategy Performance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "backtest_total_return{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "Total Return",
            "refId": "A"
          },
          {
            "expr": "backtest_annualized_return{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "Annualized Return",
            "refId": "B"
          },
          {
            "expr": "backtest_sharpe_ratio{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "Sharpe Ratio",
            "refId": "C"
          },
          {
            "expr": "backtest_max_drawdown{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "Max Drawdown",
            "refId": "D"
          },
          {
            "expr": "backtest_calmar_ratio{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "Calmar Ratio",
            "refId": "E"
          },
          {
            "expr": "backtest_win_rate{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "Win Rate",
            "refId": "F"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": -10
                },
                {
                  "color": "yellow",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 10
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Sharpe Ratio"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": 0
                      },
                      {
                        "color": "yellow",
                        "value": 0.5
                      },
                      {
                        "color": "green",
                        "value": 1.0
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Calmar Ratio"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": 0
                      },
                      {
                        "color": "yellow",
                        "value": 0.3
                      },
                      {
                        "color": "green",
                        "value": 0.5
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Strategy Comparison - Cumulative Returns",
        "type": "timeseries",
        "targets": [
          {
            "expr": "backtest_cumulative_return{backtest_id=\"$backtest\"}",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          },
          {
            "expr": "benchmark_cumulative_return{benchmark=\"SPY\"}",
            "legendFormat": "S&P 500 Benchmark",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "Cumulative Return (%)",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "S&P 500 Benchmark"
              },
              "properties": [
                {
                  "id": "custom.lineWidth",
                  "value": 1
                },
                {
                  "id": "custom.lineStyle",
                  "value": {
                    "dash": [
                      10,
                      10
                    ],
                    "fill": "dash"
                  }
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "gray"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": [
              "lastNotNull",
              "max",
              "min"
            ]
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 3,
        "title": "Strategy Risk-Return Scatter Plot",
        "type": "scatterplot",
        "targets": [
          {
            "expr": "backtest_risk_return_scatter{backtest_id=\"$backtest\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Volatility"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "bottom"
                },
                {
                  "id": "custom.axisLabel",
                  "value": "Volatility (%)"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Return"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "left"
                },
                {
                  "id": "custom.axisLabel",
                  "value": "Annualized Return (%)"
                }
              ]
            }
          ]
        },
        "options": {
          "series": [
            {
              "pointSize": {
                "fixed": 10
              },
              "pointColor": {
                "field": "Strategy"
              },
              "label": "Strategy",
              "x": "Volatility",
              "y": "Return"
            }
          ],
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 4,
        "title": "Drawdown Comparison",
        "type": "timeseries",
        "targets": [
          {
            "expr": "backtest_drawdown{backtest_id=\"$backtest\"}",
            "legendFormat": "{{strategy}} Drawdown",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": -5
                },
                {
                  "color": "red",
                  "value": -10
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Rolling Sharpe Ratio Comparison",
        "type": "timeseries",
        "targets": [
          {
            "expr": "backtest_rolling_sharpe{backtest_id=\"$backtest\", window=\"90d\"}",
            "legendFormat": "{{strategy}} Rolling Sharpe",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 1,
              "fillOpacity": 0
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 1.0
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Strategy Performance Metrics Table",
        "type": "table",
        "targets": [
          {
            "expr": "backtest_performance_metrics{backtest_id=\"$backtest\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "auto"
            },
            "decimals": 3
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Strategy"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 150
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Total Return"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": -10
                      },
                      {
                        "color": "yellow",
                        "value": 0
                      },
                      {
                        "color": "green",
                        "value": 10
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Sharpe Ratio"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "red",
                        "value": 0
                      },
                      {
                        "color": "yellow",
                        "value": 0.5
                      },
                      {
                        "color": "green",
                        "value": 1.0
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Max Drawdown"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": -5
                      },
                      {
                        "color": "yellow",
                        "value": -10
                      },
                      {
                        "color": "red",
                        "value": -20
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Sharpe Ratio"
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Transaction Cost Analysis",
        "type": "timeseries",
        "targets": [
          {
            "expr": "backtest_transaction_costs{backtest_id=\"$backtest\"}",
            "legendFormat": "{{strategy}} Transaction Costs",
            "refId": "A"
          },
          {
            "expr": "backtest_turnover_rate{backtest_id=\"$backtest\"}",
            "legendFormat": "{{strategy}} Turnover Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Transaction Costs"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "currencyUSD"
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Turnover Rate"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "orange"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Strategy Allocation Evolution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "backtest_allocation_history{backtest_id=\"$backtest\", strategy=\"$strategy\"}",
            "legendFormat": "{{asset}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 1,
              "fillOpacity": 30,
              "gradientMode": "none",
              "stacking": {
                "mode": "normal",
                "group": "A"
              }
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "right"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "Walk-Forward Analysis Results",
        "type": "heatmap",
        "targets": [
          {
            "expr": "backtest_walk_forward_results{backtest_id=\"$backtest\"}",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "color": {
              "mode": "spectrum",
              "scheme": "RdYlGn",
              "steps": 64
            }
          }
        },
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellValues": {
            "decimals": 2
          },
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "RdYlGn",
            "steps": 64
          },
          "legend": {
            "show": true
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Statistical Significance Tests",
        "type": "table",
        "targets": [
          {
            "expr": "backtest_statistical_tests{backtest_id=\"$backtest\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "auto"
            },
            "decimals": 4
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "P-Value"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": 0
                      },
                      {
                        "color": "yellow",
                        "value": 0.05
                      },
                      {
                        "color": "red",
                        "value": 0.1
                      }
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Significant"
              },
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {
                      "options": {
                        "true": {
                          "color": "green",
                          "index": 0,
                          "text": "Yes"
                        }
                      },
                      "type": "value"
                    },
                    {
                      "options": {
                        "false": {
                          "color": "red",
                          "index": 1,
                          "text": "No"
                        }
                      },
                      "type": "value"
                    }
                  ]
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": false,
              "displayName": "P-Value"
            }
          ]
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 11,
        "title": "Backtest Execution Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"analytics-service\"} |= \"backtest\" | json | backtest_id=\"$backtest\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 46
        }
      }
    ],
    "time": {
      "from": "now-2y",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    },
    "templating": {
      "list": [
        {
          "name": "backtest",
          "type": "query",
          "query": "label_values(backtest_total_return, backtest_id)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "options": [],
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0
        },
        {
          "name": "strategy",
          "type": "query",
          "query": "label_values(backtest_total_return{backtest_id=\"$backtest\"}, strategy)",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "multi": true,
          "options": [],
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0
        },
        {
          "name": "rebalance_frequency",
          "type": "custom",
          "options": [
            {
              "text": "Daily",
              "value": "daily",
              "selected": false
            },
            {
              "text": "Weekly",
              "value": "weekly",
              "selected": false
            },
            {
              "text": "Monthly",
              "value": "monthly",
              "selected": true
            },
            {
              "text": "Quarterly",
              "value": "quarterly",
              "selected": false
            }
          ],
          "current": {
            "text": "Monthly",
            "value": "monthly"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Backtest Start/End",
          "datasource": "Portfolio Analytics API",
          "enable": true,
          "expr": "backtest_execution_events{backtest_id=\"$backtest\"}",
          "iconColor": "blue",
          "textFormat": "{{event_type}}: {{description}}"
        },
        {
          "name": "Market Regime Changes",
          "datasource": "Portfolio Analytics API",
          "enable": true,
          "expr": "market_regime_changes",
          "iconColor": "purple",
          "textFormat": "Regime: {{regime_type}}"
        }
      ]
    },
    "refresh": "5m",
    "schemaVersion": 30,
    "version": 1
  },
  "overwrite": true
}